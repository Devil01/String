void RemoveDuplicate(string s)
{
    map<char,int>M;
    for(int i=0;i<s.size();i++)
    {
       if(M[s[i]]==0){
          cout<<s[i];  
          M[s[i]]=1;
       }
    }
}
int main()
{
    string s;
    cin>>s;
    RemoveDuplicate(s);
    return 0;
}
